    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.26.0",
    "fs": "0.0.1-security",
    "moment": "^2.29.1",
    "probot": "^12.2.2",
    "probot-config": "^1.1.0",
    "probot-scheduler": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "smee-client": "^1.2.2",
    "standard": "^16.0.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
